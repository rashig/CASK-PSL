{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "Log Parser Transform",
      "properties": [
        {
          "widget-type": "select",
          "label": "Log Format",
          "name": "logFormat",
          "widget-attributes": {
            "values": [
              "CLF",
              "S3",
              "Cloudfront"
            ],
            "default": "CLF"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Input Name",
          "name": "inputName"
        }
      ]
    }
  ],
  "outputs": [
    {
      "widget-type": "non-editable-schema-editor",
      "schema": {
        "name": "etlSchemaBody",
        "type": "record",
        "fields": [
          {
            "name": "uri",
            "type": "string"
          },
          {
            "name": "ip",
            "type": "string"
          },
          {
            "name": "browser",
            "type": "string"
          },
          {
            "name": "device",
            "type": "string"
          },
          {
            "name": "httpStatus",
            "type": "int"
          },
          {
            "name": "ts",
            "type": "long"
          }
        ]
      }
    }
  ]
}
